<div class='container'>
  <div class='row'>
    <div class='col-md-12'>
      <%= search_form_for @q, url: calculations_operator_requests_url, class: 'form-inline' do |f| %>
          <div class='form-group'>
            <%= f.text_field :date_gteq, id: 'gteq', class:'form-control', placeholder: 'Дата от' %>
          </div>
          <div class='form-group'>
            <%= f.text_field :date_lteq, id: 'lteq', class:'form-control', placeholder: 'до' %>
          </div>

          <%= f.submit 'Поиск', class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
  <div class='row'>
    <div class='col-md-12'>
      <%- if @info%>
          <h3>&lambda;:<%= @info[:lambda].round(3)%></h3>
          <h3>&mu;:<%= @info[:mu].round(0)%></h3>
          <h3>&rho;:<%= @info[:ro].round(3)%></h3>
          <h3>Pn:<%= @info[:piN].round(3)%></h3>
          <h3>M:<%= @info[:em].round(3)%></h3>
          <h3>W1:<%= (@info[:w1]*60).round(0)%></h3>
      <% else %>
          <h2>Задайте поиск</h2>
      <% end %>
    </div>
  </div>
</div>